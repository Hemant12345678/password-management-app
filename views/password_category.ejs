        <%- include('header') %>
        <div class="container">
                <div class="row">
                    <div class="user-welcome" ><a href="#">Welcome: <% if (loginUser) { %> <%= loginUser %> <% } %> </a></div>
                    <h1 class="head">Password Category Lists</h1>
                    <br>
                 <table class=" table table-striped table-primary">
                  <thead>
                    <tr>
                      <th>Password Category Name</th>
                
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(records.length>0) { 
                      records.forEach(function(row){
                      %>
                    <tr>
                      <td><%= row.password_category %></td>
                     
                      <td class="my-table"><a class="btn btn-primary btn-xs" href="/passwordCategory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
                        <a class="btn btn-danger btn-xs" href="/passwordCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
                      </td>
                    </tr>
                    <%  }) } %>
                  </tbody>
                </table>
              </div>
            </div>
  </body>
</html>